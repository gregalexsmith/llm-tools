{
  "name": "llm-tools",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "concurrently -c \"auto\" \"npm:dev:*\"",
    "dev:web": "next dev",
    "dev:worker": "node --env-file=.env --import tsx ./src/workers/worker.ts",
    "dev:redis": "docker-compose up -d --remove-orphans redis",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "db:generate": "prisma generate",
    "db:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.1",
    "@prisma/client": "^5.5.2",
    "@t3-oss/env-nextjs": "^0.7.0",
    "bullmq": "^5.0.0",
    "classnames": "^2.5.1",
    "langchain": "^0.0.210",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-push": "^3.6.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.5.2",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2"
  }
}
